    <list class = "list">
        <list-item style="width:454px;height:60px;">
        </list-item>
        <list-item class= "item-title">
                <input type = "button" style="margin-left:85px;width:130px;height:60px;" onclick = "goHome" value = "首页"/>
                <input type = "button" style="margin-left:20px;width:130px;height:60px;" onclick = "goPrevious" value = "上一页"/>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class = "item-types">
            <text class = "whole-text">通用属性</text>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class = "item-two">
            <div class = "two-div">
                <div class = "two-left-div" style="margin-left:30px;width:170px;">
                    <text class = "text">show</text>
                </div>
                <div class = "two-right-div" style = "flex-direction:column">
                    <input type = "radio" show="{{status}}" class = "checkbox"/>
                    <input type="button" class = "text" onclick=changeShow value="改变显示"></input>
                </div>
            </div>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class = "item-types">
            <text class = "whole-text">特有属性</text>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class ="item-attribute">
            <div class = attribute-left-div>
                <text class = "text">checked</text>
            </div>
            <div class = "attribute-right-div">
                <input type = "radio" class = "radio" onchange = "changeChecked"/>
                <text class = "text">值为{{show}}</text>
            </div>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class ="item-attribute">
            <div class = attribute-left-div>
                <text class = "text">name</text>
            </div>
            <div class = "attribute-right-div">
                <input type = "radio" name = "nameShow" class = "radio" checked = "true" onchange = "showName"/>
                <text class = "text">值为{{name}}</text>
            </div>
        </list-item>
        <list-item style="width:454px;height:60px;">
        </list-item>
    </list>