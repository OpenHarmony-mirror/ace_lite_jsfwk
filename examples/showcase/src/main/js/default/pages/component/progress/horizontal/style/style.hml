    <list class = "list">
        <list-item style="width:454px;height:60px;">
        </list-item>
        <list-item class= "item-title">
                <input type = "button" style="margin-left:85px;width:130px;height:60px;" onclick = "goHome" value = "首页"/>
                <input type = "button" style="margin-left:20px;width:130px;height:60px;" onclick = "goPrevious" value = "上一页"/>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class = "item-types">
            <text class = "whole-text">通用样式</text>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class = "item-three">
            <div class = "three-div">
                <div class = "three-left-div" style="margin-left:30px;width:170px;">
                    <text class = "text">背景</text>
                    <text class = "text">left为50</text>
                    <text class = "text">top为50</text>
                </div>
                <stack class = "general-stack" style="width:200px;">
                    <progress type = "horizontal" class = "general-item" percent = "40"/>
                </stack>
            </div>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class = "item-three">
            <div class = "three-div">
                <div class = "three-left-div" style="margin-left:30px;width:170px;">
                    <text class = "text">margin</text>
                    <text class = "text">height</text>
                    <text class = "text">width</text>
                </div>
                <div class = "three-right-div" style = "background-color:springgreen;width:200px;">
                    <progress type = "horizontal" class = "margin-show" style="background-color:#ff0000;" percent = "30"/>
                </div>
            </div>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class = "item-two">
            <div class = "two-div">
                <div class = "two-left-div" style="margin-left:30px;width:170px;">
                    <text class = "text">border</text>
                    <text class = "text">padding</text>
                </div>
                <div class = "two-right-div" style = "background-color:springgreen;width:200px;">
                    <progress type = "horizontal" class = "border-show" percent = "50"/>
                </div>
            </div>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class = "item-two">
            <div class = "two-div">
                <div class = "two-left-div" style="margin-left:30px;width:170px;">
                    <input type = "button" style="margin-left:10px;width:130px;height:60px;" onclick = "changeDisplay" value = "display"/>
                </div>
                <div class = "two-right-div" style = "background-color:springgreen;width:200px;">
                    <progress type = "horizontal" class = "border-show" style="display:{{displayValue}}" percent = "50"/>
                </div>
            </div>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class = "item-types">
            <text class = "whole-text">特有样式</text>
        </list-item>
        <list-item class="separatorContainerStyle">
        </list-item>
        <list-item class = "item-three">
            <div class = "three-div">
                <div class = "three-left-div" style="margin-left:30px;width:170px;">
                    <text class = "text">color</text>
                    <text class = "text" style="height:80px;width:160px;">stroke-width</text>
                </div>
                <div class = "style-right-div">
                    <progress type = "horizontal" class = "special-style" percent = "80"/>
                </div>
            </div>
        </list-item>
        <list-item style="width:454px;height:60px;">
        </list-item>
    </list>