    <list style="width:400px;height:400px;left:0px;top:0px;border-width:1px;border-color:#ffa500" onclick=clickFunc onswipe="listswipe">
        <list-item style="width:400px;height:80px;">
        </list-item>
        <list-item style="width:400px;height:80px;">
          <input type="button" style="margin-left:85px;width:130px;height:60px;" value="首页" onclick=backMain>
          </input>
          <input type="button" style="margin-left:20px;width:130px;height:60px;" value="上一页" onclick=backSuperior>
